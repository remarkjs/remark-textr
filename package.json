{
  "name": "remark-textr",
  "version": "4.0.0",
  "description": "remark plugin to integrate with Textr",
  "license": "MIT",
  "keywords": [
    "unified",
    "remark",
    "remark-plugin",
    "plugin",
    "mdast",
    "markdown",
    "text",
    "typography",
    "typographic",
    "typo",
    "textr"
  ],
  "repository": "remarkjs/remark-textr",
  "bugs": "https://github.com/remarkjs/remark-textr/issues",
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/unified"
  },
  "author": "Denys Dovhan <email@denysdovhan.com> (https://denysdovhan.com)",
  "contributors": [
    "Denys Dovhan <email@denysdovhan.com> (https://denysdovhan.com)",
    "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)",
    "Vladimir Starkov <iamstarkov@gmail.com>",
    "Andrey Polischuk <andre.polischuk@gmail.com>"
  ],
  "sideEffects": false,
  "type": "module",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "dependencies": {
    "textr": "^0.3.0",
    "unist-util-visit": "^4.0.0"
  },
  "devDependencies": {
    "c8": "^7.0.0",
    "prettier": "^2.0.0",
    "remark": "^14.0.0",
    "remark-cli": "^10.0.0",
    "remark-preset-wooorm": "^8.0.0",
    "tape": "^5.0.0",
    "typographic-ellipses": "^1.0.0",
    "typographic-quotes": "^2.0.0",
    "xo": "^0.37.0"
  },
  "scripts": {
    "format": "remark . -qfo && prettier . -w --loglevel warn && xo --fix",
    "test-api": "node --conditions development test.js",
    "test-coverage": "c8 --check-coverage --branches 100 --functions 100 --lines 100 --statements 100 --reporter lcov npm run test-api",
    "test": "npm run format && npm run test-coverage"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true,
    "rules": {
      "no-var": "off",
      "prefer-arrow-callback": "off",
      "unicorn/no-array-callback-reference": "off",
      "unicorn/string-content": "off"
    }
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm"
    ]
  }
}
